          <joint name="Gofa/J1" class="ur5e/type_joint1" axis="0 0 1"/>
          <geom name="Gofa/geom_link1" class="Gofa/visual" material="Gofa/grey" mesh="Gofa/link1" />
          <body name="Gofa/2st_link" pos="0 -0.085 0.0775" euler="0 0 0">
