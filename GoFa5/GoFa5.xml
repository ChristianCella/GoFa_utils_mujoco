<?xml version="1.0" encoding="utf-8"?>
<mujoco>
  <compiler autolimits="true" balanceinertia="true" angle="radian" coordinate="local"/> <!-- If autolimits “true”, no need to specify that joints>/actuators are limited (inferred from the range) -->
  <option integrator="implicitfast"/>
  <option gravity = "0 0 0"/> <!-- 0 0 -9.81 --> 
  <size memory="2M"/> <!-- size of memory allocated for dynamic arrays in the mjData.arena memory space, in bytes -->
  
  <visual>
    <global azimuth="120" elevation="-20"/> 
    <!-- azimuth=rotation around z-axis (0 corresponds to looking in the positive x direction); elevation=rotation around a vector parallel to the camera’s X-axis (<0 corresponds to camera up)-->
    <headlight ambient="0.10000000000000001 0.10000000000000001 0.10000000000000001" diffuse="0.59999999999999998 0.59999999999999998 0.59999999999999998" specular="0 0 0"/>
    <!--headlight is a directional light centered at the current camera and pointed in the direction in which the camera is looking (in the sense of OpenGL-->
    <rgba haze="0.14999999999999999 0.25 0.34999999999999998 1"/>
  </visual>
  
  <default>
    <default class="Gofa/">
     <material specular="0.5" shininess="0.25"/>
     <default class="ur5e/type_joint1">
       <joint range="-3.1415000000000002 3.1415000000000002"/>
       <general ctrlrange="-3.1415000000000002 3.1415000000000002"/>
     </default>
     <default class="Gofa/visual">
          <geom type="mesh" group="2"/>
     </default>
     <default class="Gofa/collision">
          <geom type="capsule" group="3"/>
          <default class="Gofa/eef_collision">
            <geom type="cylinder"/>
          </default>
     </default>
    </default>
  </default>

  <asset>
    <!-- texture and material for the ground -->
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3" markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    <!-- download the robot meshes and define colors -->
    <material name="Gofa/white" class="Gofa/" rgba="1 1 1 1" />
    <material name="Gofa/grey" class="Gofa/" rgba="0.27800000000000002 0.27800000000000002 0.27800000000000002 1" />
    <mesh name="Gofa/base" class="Gofa/" file="collision/base_link.stl" scale="1 1 1"/>
    <mesh name="Gofa/link1" class="Gofa/" file="collision/link_1.stl" scale="1 1 1"/>
    <mesh name="Gofa/link2" class="Gofa/" file="collision/link_2.stl" scale="1 1 1"/>
    <mesh name="Gofa/link3" class="Gofa/" file="collision/link_3.stl" scale="1 1 1"/>
    <mesh name="Gofa/link4" class="Gofa/" file="collision/link_4.stl" scale="1 1 1"/>
    <mesh name="Gofa/link5" class="Gofa/" file="collision/link_5.stl" scale="1 1 1"/>
    <mesh name="Gofa/link6" class="Gofa/" file="collision/link_6.stl" scale="1 1 1"/>
  </asset>
  <worldbody>
    <geom name="floor" pos="0 0 0" size="0 0 0.5" type="plane" material="groundplane"/>
    <body name="floor" pos="0 0 0">
    </body>
    <body name="Gofa/Robot">
      <body name="Gofa/Base" pos="0 0 0">
