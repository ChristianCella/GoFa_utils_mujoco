	            <joint name="Gofa/J6" range="-4.7123889804 4.7123889804" axis="1 0 0"/>
	            <geom name="Gofa/geom_link6" class="Gofa/visual" rgba="0.6 0.6 0.6 1" mesh="Gofa/link6" />
